{
  "name": "autoupdates",
  "productName": "HelloWorld",
  "author": "Cameron Nokes",
  "version": "1.0.0",
  "description": "Electron app demonstrating auto updating",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js --dev",
    "package": "build"
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "electron": "^3.0.7",
    "electron-builder": "20.31.2",
    "electron-builder-squirrel-windows": "^20.31.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "electron-squirrel-startup": "^1.0.0",
    "semver": "^5.3.0"
  },
  "build": {
    "files": "!build-assets${/*}",
    "appId": "com.Egghead.HelloWorld",
    "forceCodeSigning": true,
    "squirrelWindows": {
      "iconUrl": "https://cameronnokes.com/egghead.ico"
    },
    "win": {
      "target": ["squirrel"]
    }
  }
